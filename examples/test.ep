%% Complex Numbers

Struct Complex: Int real, Int imaginary.

Function C(Int x, Int y) -> Complex:
	Complex c,
	c_real :- x,
	c_imaginary :- y,
	return c.

Function sum(Complex a, Complex b) -> Complex:
	Complex c,
	c_real      :- a_real      + b_real,
	c_imaginary :- a_imaginary + b_imaginary,
	return c.

Function negative(Complex a) -> Complex:
	Complex c,
	c_real 		:- -a_real,
	c_imaginary :- -a_imaginary,
	return c.

Function prod(Complex a, Complex b) -> Complex:
	Complex c,
	c_real 		:- (a_real * b_real) 	 + (a_imaginary * b_imaginary),
	c_imaginary :- (a_real * b_imaginary) + (a_imaginary * b_real),
	return c.




